FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type reactingCompressibleSystem;

radiationModel none;

mixture
{
    type basic;
//     calculateDensity yes;
    thermoType
    {
        transport   const;
        thermo      hConst;
        equationOfState idealGas;
    }
    equationOfState
    {
        gamma           1.4;            // Heat capactiy ratio
    }
    specie
    {
        molWeight       11640.3;
    }
    transport
    {
        mu              0;
        Pr              1;
    }
    thermodynamics
    {
        Cp          2.5;
        Cv          1.785714286;
        Hf          0;
    }

    residualRho     1e-6;
    residualAlpha   1e-10;
}

