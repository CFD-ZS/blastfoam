FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     blastFoam;

startFrom       startTime;

startTime       0; // 0.00598000;

stopAt          endTime;

endTime         0.005;

deltaT          1e-8;

writeControl    adjustableRunTime;

writeInterval   5e-5;

cycleWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression off;

timeFormat      general; // fixed; // using precision below --- better for paraview

timePrecision   12;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.5;

maxDeltaT       1;

functions
{
    impulse
    {
        type                impulse;
        libs                ("libblastFunctionObjects.so");

        writeControl    writeTime;
        pRef            101298;
    }
    dynamicP
    {
        type                dynamicPressure;
        libs                ("libblastFunctionObjects.so");

        writeControl    writeTime;
        store yes;
    }
};

// ************************************************************************* //
