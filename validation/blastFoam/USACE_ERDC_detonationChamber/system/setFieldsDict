FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.c4 0
);

regions
(
    sphereToCell
    {
        centre (0.900 0 0);
        radius 0.0255;

        fieldValues
        (
            volScalarFieldValue alpha.c4 1
        );
    }
);


